{
  "schemaVersion": 1,
  "id": "tcosmatic",
  "version": "${version}",
  "name": "TCosmatic",
  "description": "A client-side cosmetic mod for custom capes with advanced editing features",
  "authors": [
    "Your Name"
  ],
  "contact": {
    "homepage": "https://github.com/yourusername/tcosmatic",
    "sources": "https://github.com/yourusername/tcosmatic"
  },
  "license": "MIT",
  "icon": "assets/tcosmatic/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "com.tcosmatic.TCosmaticMod"
    ],
    "client": [
      "com.tcosmatic.client.TCosmaticClient"
    ]
  },
  "mixins": [
    "tcosmatic.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.0",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ],
      "links": {
        "modmenu.discord": "https://discord.gg/yourserver"
      }
    }
  }
